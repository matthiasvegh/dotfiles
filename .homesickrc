`git clone https://github.com/gmarik/Vundle.vim.git ~/.vim/bundle/Vundle.vim`
`mkdir -p ~/.vim/autoload ~/.vim/bundle`
`curl -LSso ~/.vim/autoload/pathogen.vim https://tpo.pe/pathogen.vim`
`git clone git://github.com/altercation/vim-colors-solarized.git /tmp/solarized.git`
`mkdir -p ~/.vim/colors`
`cp /tmp/solarized.git/colors/solarized.vim ~/.vim/colors`
`vim +PluginInstall +qall`
`vim +PluginUpdate +qall`
`~/.vim/bundle/YouCompleteMe/install.sh --clang-completer`
`mkdir -p ~/.zsh/completion`
`(cd /tmp && git clone https://github.com/tj/git-extras.git && cd git-extras && git checkout $(git describe --tags $(git rev-list --tags --max-count=1)) && sudo make install)`
